("** equality **")

([1, 2] == [1, 2])
([] == [])
([1, 2, 3] == [1, 2])
([1, 2] == [])
([] == [1, 2])


("** inequality **")

([1, 2] != [1, 2])
([] != [])
([1, 2, 3] != [1, 2])
([1, 2] != [])
([] != [1, 2])


("** foreach **")

{
    // foreach without a return
    integer sum = 0;
    foreach (integer i, [1, 2, 4], ``{
	sum = sum + i;
    });
    return sum;
}

("** find **")
{
    list<integer> m = [1];
    // The expression should return a boolean
    // But nil crashes y2base
    return find (integer i, m, ``( nil ));
}


("** prepend **")

(prepend ([1, 4], 8))
(prepend ([], 8))
