.ini

`ag_ini(
  `IniAgent("debug/chg-repeat.in.test",
    $[
      "options" : [ "global_values", "repeat_names" ],
      "comments": [ "^[ \t]*;.*", ";.*", "\\{[^}]*\\}", "^[ \t]*$" ],
      "sections" : [
        $[
        "begin" : [ "[ \t]*\\+([A-Za-z0-9_]+)[ \t]*", "+%s" ],
        "end"   : [ "[ \t]*\\-([A-Za-z0-9_]+)[ \t]*", "-%s" ],
        ],
      ],
      "params" : [
        $[
        "match" : [ "^[ \t]*([^=]*[^ \t=])[ \t]*=[ \t]*(.*[^ \t]|)[ \t]*$" , "%s=%s"],
      ],
    ],
    ]
  )
)
