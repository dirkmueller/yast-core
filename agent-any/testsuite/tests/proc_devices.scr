.

`anyagent(
    SCR::AnyAgent (
    ``(File ("tests/proc_devices.in")),	// real file name
     "#\n",			// Comment
    true,				// read-only
     ``(Tuple (
      Choice (
        ["Character devices:\n",
	`char (
	  Tuple (Optional (Whitespace()), Value (Number()), Whitespace(), Name(String("^\n")), Continue ("\n"))
	)
	]
      ),
      Choice (
	["Block devices:\n",
	`block (
	  Tuple (Optional (Whitespace()), Value (Number()), Whitespace(), Name(String("^\n")), Continue ("\n"))
	)
	]
      )
     ))
   )
)
